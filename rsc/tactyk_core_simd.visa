        insertps $DEST, xTEMPA, $INSPS_arg
        xor rTEMPA, rTEMPA
        pxor xTEMPA, xTEMPA
        jmp $NEXT_INSTRUCTION
    case F32XF F32XCf
      code
        $LD_A_IMMEDIATE
        movq xTEMPB, rTEMPA
        cvtsd2ss xTEMPA, xTEMPB
        insertps $DEST, xTEMPA, $INSPS_arg
        xor rTEMPA, rTEMPA
        pxor xTEMPA, xTEMPA
        pxor xTEMPB, xTEMPB
        jmp $NEXT_INSTRUCTION
    
