        cmp rTEMPC, 0
        errorlt STATUS_ROGUE_POINTER
        cmp $ADDRESS_REGISTER, 0
        erroreq STATUS_UNBOUND_MEMORY
        lfence
        $LD_SRC_IMMEDIATE
        mov [$ADDRESS_REGISTER+rTEMPC], $TGT_SRC
        $RESET_TEMP
    
    case DADD DsADD
      sub tmp a
