  operand
    type address
    $ADDRESS_REGISTER $VALUE
  operand
    type data const integer
    $OFFSET $VALUE
    scramble rTEMPC
    $LD_OFFSET_IMMEDIATE $SC_LD_INDIRECT
  operand
