PROGRAM prg1
  BYTES:
    assign a 2
    assign b 3
    assign c 5
    assign d 7
    assign e 11
    assign f 13
    exit

PROGRAM prg2
  WORDS:
    assign a 257
    assign b 12345
    assign c 54321
    assign d 2000
    assign e 500
    assign f 65535
    exit

EXEC prg1 BYTES

TEST
  rA 2
  rB 3
  rC 5
  rD 7
  rE 11
  rF 13

EXEC prg2 WORDS

TEST
  rA 257
  rB 12345
  rC 54321
  rD 2000
  rE 500
  rF 65535











