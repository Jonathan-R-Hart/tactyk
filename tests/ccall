# TEST:  ccall
#
# ccall is a call into a c function which operates on integer arguments.
#
# Virtual-ISA: tactyk_core
# Templates:  tcall (Cfunc)

PROGRAM
  MAIN:
    assign a 6
    assign b 5
    assign c 4
    assign d 3
    assign e 2
    assign f 1
    ccall cfunc-1
    assign a 100
    ccall cfunc-2
    ccall cfunc-4
    ccall cfunc-3
    exit

EXEC MAIN
  TEST
    callback 1
    status 1
    rA 6
    rB 5
    rC 4
    rD 3
    rE 2
    rF 1
    arg0 6
    arg1 5
    arg2 4
    arg3 3
    arg4 2
    arg5 1
  CONTINUE
  TEST
    callback 2
    status 1
    rA 100
    arg0 100
  CONTINUE
  TEST
    callback 4
    status 1
  CONTINUE
  TEST
    status 1
    callback 3
  CONTINUE

TEST

