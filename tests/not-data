# TEST:  bitwise not
# 
# Virtual-ISA: tactykcore
# Templates:  not data (D)

PROGRAM  
  MAIN:
    assign a b.1111111100000000111111110000000011111111000000001111111100000000
    assign b b.0000111111110000000011111111000000001111111100000000111111110000
    assign c b.1001010010101001101010010010101001111100100101110100101010101001
    assign d b.1111111111111111111111111111111111111111111111111111111111111111
    assign e 0
    assign f 1
    not a
    not b
    not c
    not d
    not e
    not f
    
    exit

EXEC MAIN

TEST
  rA b.0000000011111111000000001111111100000000111111110000000011111111
  rB b.1111000000001111111100000000111111110000000011111111000000001111
  rC b.0110101101010110010101101101010110000011011010001011010101010110
  rD 0
  rE b.1111111111111111111111111111111111111111111111111111111111111111
  rF b.1111111111111111111111111111111111111111111111111111111111111110
