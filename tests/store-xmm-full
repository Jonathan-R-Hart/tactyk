# TEST:  Write full (128-bit) contents of xmmx registers
# 
# Virtual-ISA: tactyk_core
# 
# Coverage:
#   store oword-addr-data-xmmx    (B128ADX)
#   store oword-addr-data-xmmx    (B128AIX)
#   store oword-addr-data-xmmx    (B128ACX)

PROGRAM
  struct s1
    2048 data
  mem m1 s1 1
  
  const C0 0
  const C16 16
  const C32 32
  const C48 48
  const C64 64
  const C80 80
  const C96 96
  const C112 112
  const C128 128
  const C144 144
  const C160 160
  const C176 176
  const C192 192
  
  INIT:
    bind addr1 m1
    exit
  
  TEST_REG:
    assign xA 1.23
    assign xB 2.34
    assign xC 3.45
    assign xD 4.56
    assign xE 5.67
    assign xF 6.78
    assign xG 7.89
    assign xH 8.90
    assign xI 9.01
    assign xJ 10.98
    assign xK 11.99
    assign xL 12.00
    assign xM 13.01
    insert xA qword.1 111.0
    insert xB qword.1 222.0
    insert xC qword.1 333.0
    insert xD qword.1 444.0
    insert xE qword.1 555.0
    insert xF qword.1 666.0
    insert xG qword.1 777.0
    insert xH qword.1 888.0
    insert xI qword.1 999.0
    insert xJ qword.1 101010.0
    insert xK qword.1 111111.0
    insert xL qword.1 222222.0
    insert xM qword.1 333333.0
    assign a 0
    store oword addr1 rA xA
    add rA 16
    store oword addr1 rA xB
    add rA 16
    store oword addr1 rA xC
    add rA 16
    store oword addr1 rA xD
    add rA 16
    store oword addr1 rA xE
    add rA 16
    store oword addr1 rA xF
    add rA 16
    store oword addr1 rA xG
    add rA 16
    store oword addr1 rA xH
    add rA 16
    store oword addr1 rA xI
    add rA 16
    store oword addr1 rA xJ
    add rA 16
    store oword addr1 rA xK
    add rA 16
    store oword addr1 rA xL
    add rA 16
    store oword addr1 rA xM
    exit
  
  TEST_INT:
    mul xA 3.33
    mul xB 3.33
    mul xC 3.33
    mul xD 3.33
    mul xE 3.33
    mul xF 3.33
    mul xG 3.33
    mul xH 3.33
    mul xI 3.33
    mul xJ 3.33
    mul xK 3.33
    mul xL 3.33
    mul xM 3.33
    insert xA qword.1 1111.0
    insert xB qword.1 1222.0
    insert xC qword.1 1333.0
    insert xD qword.1 1444.0
    insert xE qword.1 1555.0
    insert xF qword.1 1666.0
    insert xG qword.1 1777.0
    insert xH qword.1 1888.0
    insert xI qword.1 1999.0
    insert xJ qword.1 1101010.0
    insert xK qword.1 1111111.0
    insert xL qword.1 1222222.0
    insert xM qword.1 1333333.0
    store oword addr1 0 xA
    store oword addr1 16 xB
    store oword addr1 32 xC
    store oword addr1 48 xD
    store oword addr1 64 xE
    store oword addr1 80 xF
    store oword addr1 96 xG
    store oword addr1 112 xH
    store oword addr1 128 xI
    store oword addr1 144 xJ
    store oword addr1 160 xK
    store oword addr1 176 xL
    store oword addr1 192 xM
    exit
  
  TEST_CONST:
    mul xA 100
    mul xB 100
    mul xC 100
    mul xD 100
    mul xE 100
    mul xF 100
    mul xG 100
    mul xH 100
    mul xI 100
    mul xJ 100
    mul xK 100
    mul xL 100
    mul xM 100
    insert xA qword.1 2111.0
    insert xB qword.1 2222.0
    insert xC qword.1 2333.0
    insert xD qword.1 2444.0
    insert xE qword.1 2555.0
    insert xF qword.1 2666.0
    insert xG qword.1 2777.0
    insert xH qword.1 2888.0
    insert xI qword.1 2999.0
    insert xJ qword.1 2101010.0
    insert xK qword.1 2111111.0
    insert xL qword.1 2222222.0
    insert xM qword.1 2333333.0
    store oword addr1 C0 xA
    store oword addr1 C16 xB
    store oword addr1 C32 xC
    store oword addr1 C48 xD
    store oword addr1 C64 xE
    store oword addr1 C80 xF
    store oword addr1 C96 xG
    store oword addr1 C112 xH
    store oword addr1 C128 xI
    store oword addr1 C144 xJ
    store oword addr1 C160 xK
    store oword addr1 C176 xL
    store oword addr1 C192 xM
    exit

EXEC INIT
TEST
  addr1 m1 0

EXEC TEST_REG
TEST
  rA 192
  xA 1.23 111.0
  xB 2.34 222.0
  xC 3.45 333.0
  xD 4.56 444.0
  xE 5.67 555.0
  xF 6.78 666.0
  xG 7.89 777.0
  xH 8.90 888.0
  xI 9.01 999.0
  xJ 10.98 101010.0
  xK 11.99 111111.0
  xL 12.00 222222.0
  xM 13.01 333333.0
  mem m1 0 float64 1.23
  mem m1 16 float64 2.34
  mem m1 32 float64 3.45
  mem m1 48 float64 4.56
  mem m1 64 float64 5.67
  mem m1 80 float64 6.78
  mem m1 96 float64 7.89
  mem m1 112 float64 8.90
  mem m1 128 float64 9.01
  mem m1 144 float64 10.98
  mem m1 160 float64 11.99
  mem m1 176 float64 12.00
  mem m1 192 float64 13.01
  mem m1 8 float64 111.0
  mem m1 24 float64 222.0
  mem m1 40 float64 333.0
  mem m1 56 float64 444.0
  mem m1 72 float64 555.0
  mem m1 88 float64 666.0
  mem m1 104 float64 777.0
  mem m1 120 float64 888.0
  mem m1 136 float64 999.0
  mem m1 152 float64 101010.0
  mem m1 168 float64 111111.0
  mem m1 184 float64 222222.0
  mem m1 200 float64 333333.0

EXEC TEST_INT
TEST
  xA 4.0959 1111.0
  xB 7.7922 1222.0
  xC 11.4885 1333.0
