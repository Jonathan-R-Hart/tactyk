PROGRAM
  CCC:
    goto DDD
    assign a 23454
  
  SPHAGETTOMAX:
    assign a 1
    goto BBB
    assign a 1234
  EEE:
    goto DONE
    bind addr1 99999
  BBB:
    goto CCC
    assign a 4321
  
  DDD:
    goto EEE
    exit
  DONE:
    exit
    
EXEC SPHAGETTOMAX

TEST
  rA 1
