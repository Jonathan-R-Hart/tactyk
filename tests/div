# TEST:  division
# 
# Virtual-ISA: tactyk_core
# Templates:  div data-data (DD), div data-integer (AI) div data-const (AC)

PROGRAM
  const C10 10
  const CN20 -20
  const C30 30
  const CN40 -40
  const C50 50
  const CN60 -60

  DO_INTS:
    assign a 999999999999999999
    div a 2
    div a 3
    div a 4
    div a 5
    div a 6
    div a 7
    div a 8
    div a 9
    div a 10
    assign b a
    div b -100
    exit
  
  DO_CONSTS:
    assign a 100000000
    assign b 200000000
    assign c 300000000
    assign d 400000000
    assign e 500000000
    assign f 600000000
    div a C10
    div b CN20
    div c C30
    div d CN40
    div e C50
    div f CN60
    exit
  
  DO_REGS:
    assign f 10
    div a f
    sub f 1
    div b f
    sub f 1
    div c f
    sub f 1
    div d f
    sub f 1
    div e f
    exit

  ONE:
    div a a
    div b b
    div c c
    div d d
    div e e
    div f f
    exit

EXEC DO_INTS

TEST
  rA 275573192239
  rB -2755731922

EXEC DO_CONSTS
TEST
  rA 10000000
  rB -10000000
  rC 10000000
  rD -10000000
  rE 10000000
  rF -10000000

EXEC DO_REGS
TEST
  rA 1000000
  rB -1111111
  rC 1250000
  rD -1428571
  rE 1666666
  rF 6

EXEC ONE
TEST
  rA 1
  rB 1
  rC 1
  rD 1
  rE 1
  rF 1




