# TEST:  bitwise xor
# 
# Virtual-ISA: tactykcore
# Templates:  xor data-data (DD), xor data-integer (AI) xor data-const (AC)

PROGRAM
  const CX  b.1111111111111111111111111111111111111111111111111111111111111111
  
  const CD1 b.1010101011110011111010111110110001111011111111101110111011101110
  const CD2 b.0010101000010101111100100100010111100001110011001110101101010000
  #           0111111100011001111001100101011001100101110011011111101001000001
  
  const CE1 b.0000000000001110111100100010100101111111111111110011110001101010
  const CE2 b.0101101111111110001010001010001010100010010001010111101010001001
  #           1010010000001111001001010111010000100010010001011011100100011100
  
  const CF1 b.1010100100100100101001010100000010110100101010010100101001010010
  const CF2 b.1111111110000000000000011111101111111100000000000011111111100010
  #           1010100101011011010110110100010010110111010101101000101001001111
  
  DO_INTS:
    assign a  b.1111111111111111111111111111111111111111111111111111111111111111
    xor a     b.1111111100000000111111110000000011111111000000001111111100000000
    xor a     b.1111111100000000000000000000000011111111000000000000000000000000
    xor a     b.1111111100000000000000000000000000000000000000000000000000000000
    #         b.0000000011111111000000001111111111111111111111110000000011111111
    
    assign b  b.1111111111111111111111111111111111111111111111111111111111111111
    xor b     b.1111111100000000111111110000000011111111000000001111111100000000
    xor b     b.0000111111110000000011111111000000001111111100000000111111110000
    xor b     b.0011111111000000001111111100000000111111110000000011111111000000
    #         b.0011000011001111001100001100111100110000110011110011000011001111
    
    assign c  b.1111111111111111111111111111111111111111111111111111111111111111
    xor c     b.1001010010101001101010010010101001111100100101110100101010101001
    xor c     b.0010100010101010101001010001111010101101010010101111100101010001
    xor c     b.1001000101011010101010011110101101101010101001110101011000001000
    #         b.1101001010100110010110100010000001000100100001010001101000001111
    
    exit
  
  DO_CONSTS:
    assign d CX
    xor d CD1
    xor d CD2
    assign e CX
    xor e CE1
    xor e CE2
    assign f CX
    xor f CF1
    xor f CF2
    exit

  DO_REGS:
    xor a d
    xor b e
    xor c f
    xor d b
    xor e c
    xor f a
    exit

EXEC DO_INTS

TEST
  rA b.0000000011111111000000001111111111111111111111110000000011111111
  rB b.0011000011001111001100001100111100110000110011110011000011001111
  rC b.1101001010100110010110100010000001000100100001010001101000001111

EXEC DO_CONSTS
TEST
  rD b.0111111100011001111001100101011001100101110011011111101001000001
  rE b.1010010000001111001001010111010000100010010001011011100100011100
  rF b.1010100101011011010110110100010010110111010101101000101001001111

EXEC DO_REGS
TEST  
  #   b.0000000011111111000000001111111111111111111111110000000011111111
  #   b.0111111100011001111001100101011001100101110011011111101001000001
  rA  b.0111111111100110111001101010100110011010001100101111101010111110
  
  #   b.0011000011001111001100001100111100110000110011110011000011001111
  #   b.1010010000001111001001010111010000100010010001011011100100011100
  rB  b.1001010011000000000101011011101100010010100010101000100111010011
  
  #   b.1101001010100110010110100010000001000100100001010001101000001111
  #   b.1010100101011011010110110100010010110111010101101000101001001111
  rC  b.0111101111111101000000010110010011110011110100111001000001000000
  
  #   b.0111111100011001111001100101011001100101110011011111101001000001
  #   b.1001010011000000000101011011101100010010100010101000100111010011
  rD  b.1110101111011001111100111110110101110111010001110111001110010010
  
  #   b.1010010000001111001001010111010000100010010001011011100100011100
  #   b.0111101111111101000000010110010011110011110100111001000001000000
  rE  b.1101111111110010001001000001000011010001100101100010100101011100
  
  #   b.1010100101011011010110110100010010110111010101101000101001001111
  #   b.0111111111100110111001101010100110011010001100101111101010111110
  rF  b.1101011010111101101111011110110100101101011001000111000011110001
