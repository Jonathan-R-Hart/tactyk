# Immedaite assignment test
PROGRAM
  ZERO:
    assign a 0
    assign b 0
    assign c 0
    assign d 0
    assign e 0
    assign f 0
    exit
  
  BYTES:
    assign a 1
    assign b 33
    assign c 40
    assign d 60
    assign e 101
    assign f 255
    exit
  
  WORDS:
    assign a 256
    assign b 12345
    assign c 54321
    assign d 2000
    assign e 500
    assign f 65535
    exit
  
  DWORDS:
    assign a 1000000000
    assign b 65536
    assign c 99999999
    assign d 123456
    assign e 789000000
    assign f 4294967295
    exit
  
  QWORDS:
    assign a -9000000000000
    assign b -123123123123
    assign c 9223372036854775808
    assign d 4294967296
    assign e -1
    assign f 9223372036854775808
    # assign xa 0.0001
    exit

EXEC BYTES

TEST
  rA 1
  rB 33
  rC 40
  rD 60
  rE 101
  rF 255

EXEC ZERO

TEST
  rA 0
  rB 0
  rC 0
  rD 0
  rE 0
  rF 0

EXEC WORDS

TEST
  rA 256
  rB 12345
  rC 54321
  rD 2000
  rE 500
  rF 65535

EXEC DWORDS

TEST
  rA 1000000000
  rB 65536
  rC 99999999
  rD 123456
  rE 789000000
  rF 4294967295

EXEC QWORDS

TEST
  rA -9000000000000
  rB -123123123123
  rC 9223372036854775808
  rD 4294967296
  rE -1
  rF 9223372036854775808
  










